AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,kEAAA,MAAA,QAAA,CAAA,IAAA,GAAA,UAAA,GAAA,MAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,CAAA,GAAA,QAAA,GAAA,MAAA,QAAA,CAAA,IAAA;AACA,gBAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,eAAA;AACA,gBAAA,IAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,GAAA,IAAA;AACA,aAFA,MAEA;AACA,oBAAA,MAAA,GAAA,KAAA;AACA;AACA,WANA;AAOA,SATA,MASA;AACA,iBAAA,KAAA;AACA;AACA,OAbA;AAcA,KAhBA;AAiBA,aAjBA,qBAiBA,QAjBA,EAiBA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,WAAA;AACA;AAnBA;AAvCA,CAAA","sourcesContent":["<template>\r\n<div class=\"ctn\">\r\n  <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n    <el-form-item label=\"活动名称\" prop=\"name\">\r\n      <el-input v-model=\"ruleForm.name\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item label=\"活动性质\" prop=\"type\">\r\n      <el-checkbox-group v-model=\"ruleForm.type\">\r\n        <el-checkbox label=\"美食/餐厅线上活动\" name=\"type\"></el-checkbox>\r\n        <el-checkbox label=\"地推活动\" name=\"type\"></el-checkbox>\r\n        <el-checkbox label=\"线下主题活动\" name=\"type\"></el-checkbox>\r\n        <el-checkbox label=\"单纯品牌曝光\" name=\"type\"></el-checkbox>\r\n      </el-checkbox-group>\r\n    </el-form-item>\r\n    <el-form-item label=\"活动形式\" prop=\"desc\">\r\n      <el-input type=\"textarea\" v-model=\"ruleForm.desc\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item>\r\n      <el-button class=\"confirm\" type=\"primary\" @click=\"submitForm('ruleForm')\">立即创建</el-button>\r\n      <el-button class=\"reset\" @click=\"resetForm('ruleForm')\">重置</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n</div>  \r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios'\r\n  export default {\r\n    data() {\r\n      return {\r\n        ruleForm: this.initFormData,\r\n        rules: {\r\n          name: [\r\n            { required: true, message: '请输入活动名称', trigger: 'blur' },\r\n            { min: 3, max: 5, message: '长度在 3 到 5 个字符', trigger: 'blur' }\r\n          ],\r\n          type: [\r\n            { type: 'array', required: true, message: '请至少选择一个活动性质', trigger: 'change' }\r\n          ],\r\n          desc: [\r\n            { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n          ]\r\n        },\r\n        sucess: false\r\n      };\r\n    },\r\n    props:{\r\n      initFormData: {\r\n        type: Object,\r\n        default: () => {\r\n          {\r\n            name: '';\r\n            type: [];\r\n            desc: ''\r\n          }\r\n        }\r\n      }\r\n    },\r\n    watch: {\r\n      initFormData: {\r\n        handler(n, o){\r\n          this.ruleForm = n\r\n        },\r\n        deep: true\r\n      }\r\n    },\r\n    methods: {\r\n      submitForm(formName) {\r\n        this.$refs[formName].validate((valid) => {\r\n          if (valid) {\r\n            let url = 'http://rap2api.taobao.org/app/mock/233956/tbl-unit-test?name=' + this.ruleForm.name + '&nature=' + this.ruleForm.type.join(',') + '&form=' + this.ruleForm.form\r\n            axios.get(url).then(res => {\r\n              if (res.status === 200) {\r\n                this.sucess = true\r\n              } else {\r\n                this.sucess= false\r\n              }\r\n            })\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      resetForm(formName) {\r\n        this.$refs[formName].resetFields();\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang='scss' scoped>\r\n  .ctn{\r\n    margin-top: 20px;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/page3/components"}]}